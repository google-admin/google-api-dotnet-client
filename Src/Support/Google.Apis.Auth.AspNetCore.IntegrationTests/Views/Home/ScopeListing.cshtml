
@model IReadOnlyList<string>
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Integreation tests - Scope Listing</title>
</head>
<body>
    <h1>Integration tests - Scope Listing</h1>
    <ul>
        @foreach (var scope in @Model)
        {
            <li>@scope</li>
        }
    </ul>
    @if (@Model.Count > 0)
    {
        <p>Test passed</p>
    }
    else
    {
        <p>Test failed</p>
    }
</body>
</html>
